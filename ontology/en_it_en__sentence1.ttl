@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_it_en_sentence1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vn.role:Theme rdfs:label "in" .

en_it_en_sentence1:Again rdfs:label "again" .

en_it_en_sentence1:Friday rdfs:label "Friday" ;
    boxer:possibleType time:Interval ;
    en_it_en_sentence1:at "1999-12-17"^^xsd:date .

en_it_en_sentence1:Of rdfs:label "of" .

en_it_en_sentence1:On rdfs:label "on" .

en_it_en_sentence1:Rel rdfs:label "a" .

en_it_en_sentence1:To rdfs:label "to" .

en_it_en_sentence1:declare_1 a en_it_en_sentence1:Declare ;
    rdfs:label "declare" ;
    vn.role:Agent en_it_en_sentence1:person_1 ;
    vn.role:Theme en_it_en_sentence1:situation_1 .

dbpedia:European_Parliament rdfs:subClassOf foaf:Organisation .

en_it_en_sentence1:Declare rdfs:label "declare" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence1:European_parliament rdfs:label "European Parliament" ;
    owl:equivalentClass dbpedia:European_Parliament .

en_it_en_sentence1:Festive a owl:Class ;
    rdfs:label "festive" ;
    rdfs:subClassOf dul:Quality .

en_it_en_sentence1:FestivePeriod a owl:Class ;
    rdfs:label "Festive Period" ;
    dul:hasQuality en_it_en_sentence1:Festive ;
    rdfs:subClassOf en_it_en_sentence1:Period .

en_it_en_sentence1:Have rdfs:label "had",
        "have" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence1:Hope rdfs:label "hope" .

en_it_en_sentence1:Interrupt rdfs:label "interrupted" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence1:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence1:New rdfs:label "New" ;
    boxer:possibleType owl:Thing ;
    dul:hasQuality en_it_en_sentence1:Happy .

en_it_en_sentence1:Once rdfs:label "once" .

en_it_en_sentence1:Period a owl:Class ;
    rdfs:label "period" .

en_it_en_sentence1:Pleasant rdfs:label "pleasant" .

en_it_en_sentence1:PleasantFestivePeriod a owl:Class ;
    rdfs:label "pleasant festive period" ;
    rdfs:subClassOf en_it_en_sentence1:FestivePeriod .

en_it_en_sentence1:Resume rdfs:label "resuming" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence1:Session rdfs:label "session" .

en_it_en_sentence1:Wish rdfs:label "wish" .

en_it_en_sentence1:Year a owl:Class ;
    rdfs:label "Year" ;
    owl:equivalentClass dbpedia:New_Year .

en_it_en_sentence1:european_parliament_1 a en_it_en_sentence1:European_parliament ;
    rdfs:label "European Parliament" ;
    quant:hasDeterminer quant:the .

en_it_en_sentence1:have_1 a en_it_en_sentence1:Have ;
    rdfs:label "had" ;
    vn.role:Agent en_it_en_sentence1:person_5 ;
    vn.role:Patient en_it_en_sentence1:period_1 .

en_it_en_sentence1:hope_1 a en_it_en_sentence1:Hope ;
    rdfs:label "hope" ;
    boxer:theme en_it_en_sentence1:have_1 ;
    quant:hasDeterminer quant:the .

en_it_en_sentence1:interrupt_1 a en_it_en_sentence1:Interrupt ;
    rdfs:label "interrupted" ;
    boxer:patient en_it_en_sentence1:session_1 ;
    en_it_en_sentence1:on "1999-12-17"^^xsd:date .

en_it_en_sentence1:like_1 a en_it_en_sentence1:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    dul:hasQuality en_it_en_sentence1:Once ;
    vn.role:Experiencer en_it_en_sentence1:person_3 ;
    vn.role:Predicate en_it_en_sentence1:person_4 ;
    vn.role:Theme en_it_en_sentence1:hope_1 ;
    en_it_en_sentence1:againTo en_it_en_sentence1:wish_1 .

en_it_en_sentence1:period_1 a en_it_en_sentence1:PleasantFestivePeriod ;
    rdfs:label "period" ;
    dul:hasQuality en_it_en_sentence1:Pleasant ;
    quant:hasDeterminer quant:a .

en_it_en_sentence1:person_1 a en_it_en_sentence1:Person ;
    rdfs:label "I" .

en_it_en_sentence1:person_2 a en_it_en_sentence1:Person ;
    rdfs:label "I" .

en_it_en_sentence1:person_3 a en_it_en_sentence1:Person ;
    rdfs:label "I" .

en_it_en_sentence1:person_4 a en_it_en_sentence1:Person ;
    rdfs:label "you" ;
    dul:associatedWith en_it_en_sentence1:year_1 .

en_it_en_sentence1:person_5 a en_it_en_sentence1:Person ;
    rdfs:label "you" .

en_it_en_sentence1:resume_1 a en_it_en_sentence1:Resume ;
    rdfs:label "resuming" ;
    vn.role:Agent en_it_en_sentence1:person_2 ;
    vn.role:Theme en_it_en_sentence1:session_1 .

en_it_en_sentence1:situation_1 a en_it_en_sentence1:Situation ;
    boxing:involves en_it_en_sentence1:interrupt_1,
        en_it_en_sentence1:like_1,
        en_it_en_sentence1:resume_1,
        en_it_en_sentence1:wish_1,
        en_it_en_sentence1:year_1 .

en_it_en_sentence1:Happy rdfs:label "happy" .

en_it_en_sentence1:session_1 a en_it_en_sentence1:Session ;
    rdfs:label "session" ;
    quant:hasDeterminer quant:the ;
    en_it_en_sentence1:sessionOf en_it_en_sentence1:european_parliament_1 .

en_it_en_sentence1:wish_1 a en_it_en_sentence1:Wish ;
    rdfs:label "wish" .

en_it_en_sentence1:year_1 a en_it_en_sentence1:Year ;
    rdfs:label "Year" ;
    dul:associatedWith en_it_en_sentence1:New ;
    dul:hasQuality en_it_en_sentence1:Happy .

en_it_en_sentence1:Person rdfs:label "I",
        "you" .

