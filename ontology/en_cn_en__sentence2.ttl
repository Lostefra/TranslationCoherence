@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix coref: <http://www.ontologydesignpatterns.org/ont/cnlp/coref.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_cn_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_cn_en__sentence2/> .
@prefix en_de_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_de_en__sentence2/> .
@prefix en_it_en_sentence1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence1/> .
@prefix en_it_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence2/> .
@prefix en_it_en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence3/> .
@prefix en_it_en_sentence4: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence4/> .
@prefix en_it_en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence5/> .
@prefix en_sentence1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence1/> .
@prefix en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence2/> .
@prefix en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence3/> .
@prefix en_sentence4: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence4/> .
@prefix en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence5/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix tc_vocabulary: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/vocabulary/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix transl_coher: <http://example.org/translation_coherence/> .
@prefix transl_coher_final: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vn.role:Cause rdfs:label "from" .

en_cn_en_sentence2:Although rdfs:label "Although" .

en_cn_en_sentence2:As rdfs:label "as" .

en_cn_en_sentence2:In rdfs:label "in" .

en_cn_en_sentence2:Of rdfs:label "of" .

en_cn_en_sentence2:millennium_1 a en_cn_en_sentence2:Millennium ;
    rdfs:label "millennium" ;
    dul:associatedWith en_cn_en_sentence2:bug_1 ;
    quant:hasDeterminer quant:the .

en_cn_en_sentence2:situation_1 a en_cn_en_sentence2:Situation ;
    boxing:involves en_cn_en_sentence2:Appear,
        en_cn_en_sentence2:See,
        en_cn_en_sentence2:appear_1,
        en_cn_en_sentence2:see_1 ;
    en_cn_en_sentence2:although en_cn_en_sentence2:suffer_1 .

en_cn_en_sentence2:Bug a owl:Class ;
    rdfs:label "bug" .

en_cn_en_sentence2:Country rdfs:label "countries" .

en_cn_en_sentence2:Disaster rdfs:label "disasters" .

en_cn_en_sentence2:Dreaded a owl:Class ;
    rdfs:label "dreaded" ;
    rdfs:subClassOf dul:Quality .

en_cn_en_sentence2:DreadedMillennium a owl:Class ;
    dul:hasQuality en_cn_en_sentence2:Dreaded ;
    rdfs:subClassOf en_cn_en_sentence2:Millennium .

en_cn_en_sentence2:DreadedMillenniumBug a owl:Class ;
    rdfs:label "Dreaded Millennium Bug" ;
    dul:associatedWith en_cn_en_sentence2:DreadedMillennium ;
    rdfs:subClassOf en_cn_en_sentence2:MillenniumBug .

en_cn_en_sentence2:MillenniumBug a owl:Class ;
    rdfs:label "Millennium Bug" ;
    dul:associatedWith en_cn_en_sentence2:Millennium ;
    rdfs:subClassOf en_cn_en_sentence2:Bug ;
    owl:equivalentClass dbpedia:Year_2000_problem .

en_cn_en_sentence2:Natural rdfs:label "natural" ;
    dul:hasQuality en_cn_en_sentence2:Truly .

en_cn_en_sentence2:NaturalDisaster rdfs:subClassOf en_cn_en_sentence2:Disaster .

en_cn_en_sentence2:People rdfs:label "people" .

en_cn_en_sentence2:Person rdfs:label "you" .

en_cn_en_sentence2:Series rdfs:label "series" .

en_cn_en_sentence2:Still rdfs:label "still" .

en_cn_en_sentence2:StillSuffer rdfs:subClassOf en_cn_en_sentence2:Suffer .

en_cn_en_sentence2:Suffer rdfs:label "suffering" ;
    rdfs:subClassOf dul:Event .

en_cn_en_sentence2:Terrible rdfs:label "terrible" ;
    dul:hasQuality en_cn_en_sentence2:Truly .

en_cn_en_sentence2:TerribleNaturalDisaster rdfs:subClassOf en_cn_en_sentence2:NaturalDisaster .

en_cn_en_sentence2:TrulyTerribleNaturalDisaster rdfs:subClassOf en_cn_en_sentence2:TerribleNaturalDisaster .

en_cn_en_sentence2:appear_1 a en_cn_en_sentence2:Appear ;
    rdfs:label "appeared" ;
    boxing:hasTruthValue boxing:False ;
    vn.role:Theme en_cn_en_sentence2:bug_1 ;
    en_cn_en_sentence2:as en_cn_en_sentence2:see_1 .

en_cn_en_sentence2:country_1 a en_cn_en_sentence2:Country ;
    rdfs:label "countries" ;
    quant:hasQuantifier quant:multiple,
        quant:some .

en_cn_en_sentence2:disaster_1 a en_cn_en_sentence2:TrulyTerribleNaturalDisaster ;
    rdfs:label "disasters" ;
    dul:hasQuality en_cn_en_sentence2:Natural,
        en_cn_en_sentence2:Terrible ;
    quant:hasQuantifier quant:multiple .

en_cn_en_sentence2:people_1 a en_cn_en_sentence2:People ;
    rdfs:label "people" ;
    quant:hasQuantifier quant:multiple ;
    en_cn_en_sentence2:in en_cn_en_sentence2:country_1 .

en_cn_en_sentence2:person_1 a en_cn_en_sentence2:Person ;
    rdfs:label "you" .

en_cn_en_sentence2:series_1 a en_cn_en_sentence2:Series ;
    rdfs:label "series" ;
    quant:hasDeterminer quant:a ;
    en_cn_en_sentence2:seriesOf en_cn_en_sentence2:disaster_1 .

en_cn_en_sentence2:suffer_1 a en_cn_en_sentence2:StillSuffer ;
    rdfs:label "suffering" ;
    dul:hasQuality en_cn_en_sentence2:Still ;
    vn.role:Cause en_cn_en_sentence2:series_1 ;
    vn.role:Experiencer en_cn_en_sentence2:people_1 .

en_cn_en_sentence2:Appear rdfs:label "appeared" ;
    rdfs:subClassOf dul:Event .

en_cn_en_sentence2:See rdfs:label "see" ;
    rdfs:subClassOf dul:Event .

en_cn_en_sentence2:Truly rdfs:label "truly" .

en_cn_en_sentence2:bug_1 a en_cn_en_sentence2:DreadedMillenniumBug ;
    rdfs:label "bug" .

en_cn_en_sentence2:see_1 a en_cn_en_sentence2:See ;
    rdfs:label "see" ;
    boxing:hasModality boxing:Possible ;
    vn.role:Agent en_cn_en_sentence2:person_1 .

en_cn_en_sentence2:Millennium a owl:Class ;
    rdfs:label "millennium" .

