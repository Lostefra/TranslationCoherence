@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

en_de_en_sentence2:Although rdfs:label "Although" .

en_de_en_sentence2:As rdfs:label "as" .

en_de_en_sentence2:In rdfs:label "in" .

en_de_en_sentence2:Nevertheless rdfs:label "nevertheless" .

en_de_en_sentence2:Of rdfs:label "of" .

en_de_en_sentence2:millennium_1 a en_de_en_sentence2:Millennium ;
    rdfs:label "millennium" ;
    dul:associatedWith en_de_en_sentence2:bug_1 ;
    quant:hasDeterminer quant:the .

en_de_en_sentence2:number_1 a en_de_en_sentence2:Number ;
    rdfs:label "number" ;
    quant:hasDeterminer quant:a .

en_de_en_sentence2:situation_1 a en_de_en_sentence2:Situation ;
    boxing:involves en_de_en_sentence2:Materialise,
        en_de_en_sentence2:See,
        en_de_en_sentence2:materialise_1,
        en_de_en_sentence2:see_1 ;
    en_de_en_sentence2:although en_de_en_sentence2:suffer_1 .

en_de_en_sentence2:Bug a owl:Class ;
    rdfs:label "bug" .

en_de_en_sentence2:Country rdfs:label "countries" .

en_de_en_sentence2:Disaster rdfs:label "disasters" .

en_de_en_sentence2:Dreaded a owl:Class ;
    rdfs:label "dreaded" ;
    rdfs:subClassOf dul:Quality .

en_de_en_sentence2:DreadedMillennium a owl:Class ;
    dul:hasQuality en_de_en_sentence2:Dreaded ;
    rdfs:subClassOf en_de_en_sentence2:Millennium .

en_de_en_sentence2:DreadedMillenniumBug a owl:Class ;
    rdfs:label "dreaded millennium bug" ;
    dul:associatedWith en_de_en_sentence2:DreadedMillennium ;
    rdfs:subClassOf en_de_en_sentence2:MillenniumBug .

en_de_en_sentence2:MillenniumBug a owl:Class ;
    rdfs:label "millennium bug" ;
    dul:associatedWith en_de_en_sentence2:Millennium ;
    rdfs:subClassOf en_de_en_sentence2:Bug ;
    owl:equivalentClass dbpedia:Year_2000_problem .

en_de_en_sentence2:Natural rdfs:label "natural" .

en_de_en_sentence2:NaturalDisaster rdfs:subClassOf en_de_en_sentence2:Disaster .

en_de_en_sentence2:NeverthelessSuffer rdfs:subClassOf en_de_en_sentence2:Suffer .

en_de_en_sentence2:Number rdfs:label "number" .

en_de_en_sentence2:People rdfs:label "people" .

en_de_en_sentence2:Person rdfs:label "you" .

en_de_en_sentence2:Series rdfs:label "series" .

en_de_en_sentence2:Suffer rdfs:label "suffered" ;
    rdfs:subClassOf dul:Event .

en_de_en_sentence2:Terrible rdfs:label "terrible" ;
    dul:hasQuality en_de_en_sentence2:Truly .

en_de_en_sentence2:Truly rdfs:label "truly" .

en_de_en_sentence2:country_1 a en_de_en_sentence2:Country ;
    rdfs:label "countries" ;
    quant:hasQuantifier quant:multiple .

en_de_en_sentence2:disaster_1 a en_de_en_sentence2:NaturalDisaster ;
    rdfs:label "disasters" ;
    dul:hasQuality en_de_en_sentence2:Natural,
        en_de_en_sentence2:Terrible ;
    quant:hasQuantifier quant:multiple .

en_de_en_sentence2:people_1 a en_de_en_sentence2:People ;
    rdfs:label "people" ;
    quant:hasQuantifier quant:multiple ;
    en_de_en_sentence2:inNumberOf en_de_en_sentence2:country_1 .

en_de_en_sentence2:person_1 a en_de_en_sentence2:Person ;
    rdfs:label "you" .

en_de_en_sentence2:see_1 a en_de_en_sentence2:See ;
    rdfs:label "seen" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent en_de_en_sentence2:person_1 ;
    en_de_en_sentence2:as en_de_en_sentence2:materialise_1 .

en_de_en_sentence2:series_1 a en_de_en_sentence2:Series ;
    rdfs:label "series" ;
    quant:hasDeterminer quant:a ;
    en_de_en_sentence2:seriesOf en_de_en_sentence2:disaster_1 .

en_de_en_sentence2:Materialise rdfs:label "materialise" ;
    rdfs:subClassOf dul:Event .

en_de_en_sentence2:See rdfs:label "seen" ;
    rdfs:subClassOf dul:Event .

en_de_en_sentence2:bug_1 a en_de_en_sentence2:DreadedMillenniumBug ;
    rdfs:label "bug" .

en_de_en_sentence2:materialise_1 a en_de_en_sentence2:Materialise ;
    rdfs:label "materialise" ;
    boxer:agent en_de_en_sentence2:bug_1 ;
    boxing:hasTruthValue boxing:False ;
    en_de_en_sentence2:nevertheless en_de_en_sentence2:suffer_1 .

en_de_en_sentence2:suffer_1 a en_de_en_sentence2:NeverthelessSuffer ;
    rdfs:label "suffered" ;
    vn.role:Experiencer en_de_en_sentence2:people_1 ;
    vn.role:Theme en_de_en_sentence2:series_1 .

en_de_en_sentence2:Millennium a owl:Class ;
    rdfs:label "millennium" .

