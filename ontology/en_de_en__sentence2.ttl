@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_cn_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_cn_en__sentence2/> .
@prefix en_de_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_de_en__sentence2/> .
@prefix en_it_en_sentence1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence1/> .
@prefix en_it_en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence2/> .
@prefix en_it_en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence3/> .
@prefix en_it_en_sentence4: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence4/> .
@prefix en_it_en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence5/> .
@prefix en_sentence1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence1/> .
@prefix en_sentence2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence2/> .
@prefix en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence3/> .
@prefix en_sentence4: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence4/> .
@prefix en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence5/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
