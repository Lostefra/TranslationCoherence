@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_it_en_sentence4: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence4/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .

vn.role:Agent rdfs:label "by" .

en_it_en_sentence4:As rdfs:label "as" .

en_it_en_sentence4:In rdfs:label "In",
        "in" .

en_it_en_sentence4:Of rdfs:label "'s",
        "of" .

en_it_en_sentence4:On rdfs:label "on" .

en_it_en_sentence4:Rel rdfs:label "," .

en_it_en_sentence4:behalf_1 a en_it_en_sentence4:Behalf ;
    rdfs:label "behalf" .

en_it_en_sentence4:like_1 a en_it_en_sentence4:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Experiencer en_it_en_sentence4:person_1 ;
    en_it_en_sentence4:as en_it_en_sentence4:request_1 ;
    en_it_en_sentence4:in en_it_en_sentence4:country_1,
        en_it_en_sentence4:meantime_1 ;
    en_it_en_sentence4:onBehalfOf en_it_en_sentence4:victim_1 .

en_it_en_sentence4:observe_1 a en_it_en_sentence4:Observe ;
    rdfs:label "observe" ;
    boxing:hasModality boxing:Necessary .

en_it_en_sentence4:silence_1 a en_it_en_sentence4:Silence ;
    rdfs:label "silence" ;
    en_it_en_sentence4:silenceOf en_it_en_sentence4:minute_1 .

en_it_en_sentence4:situation_1 a en_it_en_sentence4:Situation ;
    boxing:involves en_it_en_sentence4:those_1 ;
    en_it_en_sentence4:entails en_it_en_sentence4:situation_2 .

en_it_en_sentence4:Behalf rdfs:label "behalf" .

en_it_en_sentence4:Country rdfs:label "countries" .

en_it_en_sentence4:European_union rdfs:label "European Union" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:European_Union .

en_it_en_sentence4:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence4:Meantime rdfs:label "meantime" .

en_it_en_sentence4:Member rdfs:label "Members" .

en_it_en_sentence4:Minute rdfs:label "minute" .

en_it_en_sentence4:Observe rdfs:label "observe" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence4:Particularly a owl:Class ;
    rdfs:label "particularly" ;
    rdfs:subClassOf dul:Quality .

en_it_en_sentence4:ParticularlyThose a owl:Class ;
    rdfs:label "Particularly Those" ;
    dul:hasQuality en_it_en_sentence4:Particularly ;
    rdfs:subClassOf en_it_en_sentence4:Those .

en_it_en_sentence4:Person rdfs:label "I" .

en_it_en_sentence4:Request rdfs:label "requested" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence4:Silence rdfs:label "silence" .

en_it_en_sentence4:Storm rdfs:label "storms" .

en_it_en_sentence4:Terrible rdfs:label "terrible" .

en_it_en_sentence4:TerribleStorm rdfs:subClassOf en_it_en_sentence4:Storm .

en_it_en_sentence4:Those a owl:Class ;
    rdfs:label "those" .

en_it_en_sentence4:Various rdfs:label "various" .

en_it_en_sentence4:VariousCountry rdfs:subClassOf en_it_en_sentence4:Country .

en_it_en_sentence4:Victim rdfs:label "victims" .

en_it_en_sentence4:country_1 a en_it_en_sentence4:VariousCountry ;
    rdfs:label "countries" ;
    dul:hasQuality en_it_en_sentence4:Various ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple ;
    en_it_en_sentence4:variousCountryOf en_it_en_sentence4:European_union .

en_it_en_sentence4:meantime_1 a en_it_en_sentence4:Meantime ;
    rdfs:label "meantime" ;
    quant:hasDeterminer quant:the .

en_it_en_sentence4:member_1 a en_it_en_sentence4:Member ;
    rdfs:label "Members" ;
    quant:hasQuantifier quant:multiple,
        quant:some .

en_it_en_sentence4:minute_1 a en_it_en_sentence4:Minute ;
    rdfs:label "minute" ;
    quant:hasDeterminer quant:a .

en_it_en_sentence4:person_1 a en_it_en_sentence4:Person ;
    rdfs:label "I" .

en_it_en_sentence4:request_1 a en_it_en_sentence4:Request ;
    rdfs:label "requested" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent en_it_en_sentence4:member_1 ;
    vn.role:Proposition en_it_en_sentence4:thing_1 .

en_it_en_sentence4:situation_2 a en_it_en_sentence4:Situation ;
    boxing:involves en_it_en_sentence4:storm_1,
        en_it_en_sentence4:those_1 .

en_it_en_sentence4:thing_1 a owl:Thing .

en_it_en_sentence4:victim_1 a en_it_en_sentence4:Victim ;
    rdfs:label "victims" ;
    dul:associatedWith en_it_en_sentence4:those_1 ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple .

en_it_en_sentence4:storm_1 a en_it_en_sentence4:TerribleStorm ;
    rdfs:label "storms" ;
    dul:hasQuality en_it_en_sentence4:Terrible ;
    quant:hasDeterminer quant:the,
        quant:those ;
    quant:hasQuantifier quant:multiple .

en_it_en_sentence4:those_1 a en_it_en_sentence4:ParticularlyThose ;
    rdfs:label "those" ;
    en_it_en_sentence4:particularlyThoseOf en_it_en_sentence4:storm_1 .

