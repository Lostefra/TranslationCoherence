@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .

en_sentence3:During rdfs:label "during" .

en_sentence3:Few rdfs:label "few" .

en_sentence3:In rdfs:label "in" .

en_sentence3:Of rdfs:label "of" .

en_sentence3:On rdfs:label "on" .

en_sentence3:course_1 a en_sentence3:Course ;
    rdfs:label "course" ;
    quant:hasDeterminer quant:the .

en_sentence3:request_1 a en_sentence3:Request ;
    rdfs:label "requested" ;
    vn.role:Agent en_sentence3:person_1 ;
    vn.role:Proposition en_sentence3:debate_1 ;
    en_sentence3:during en_sentence3:part-session_1 ;
    en_sentence3:inCourseOf en_sentence3:day_1 .

en_sentence3:Course a owl:Class ;
    rdfs:label "course" ;
    owl:equivalentClass <http://dbpedia.org/resource/Course_(education)> .

en_sentence3:Day rdfs:label "days" .

en_sentence3:Debate rdfs:label "debate" .

en_sentence3:FewDay rdfs:subClassOf en_sentence3:Day .

en_sentence3:Next rdfs:label "next" .

en_sentence3:NextFewDay rdfs:subClassOf en_sentence3:FewDay .

en_sentence3:Part-session rdfs:label "part-session" .

en_sentence3:Person rdfs:label "You" .

en_sentence3:Request rdfs:label "requested" ;
    rdfs:subClassOf dul:Event .

en_sentence3:Subject a owl:Class ;
    rdfs:label "subject" ;
    owl:equivalentClass <http://dbpedia.org/resource/Course_(education)> .

en_sentence3:day_1 a en_sentence3:NextFewDay ;
    rdfs:label "days" ;
    dul:hasQuality en_sentence3:Next ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:few,
        quant:multiple .

en_sentence3:debate_1 a en_sentence3:Debate ;
    rdfs:label "debate" ;
    quant:hasDeterminer quant:a ;
    en_sentence3:on en_sentence3:subject_1 .

en_sentence3:part-session_1 a en_sentence3:Part-session ;
    rdfs:label "part-session" ;
    quant:hasDeterminer quant:this .

en_sentence3:person_1 a en_sentence3:Person ;
    rdfs:label "You" .

en_sentence3:subject_1 a en_sentence3:Subject ;
    rdfs:label "subject" ;
    quant:hasDeterminer quant:this .

