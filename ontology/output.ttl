@prefix g1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/g1.ttl/> .
@prefix g2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/g2.ttl/> .

:expression_1 a transl_coher:Expression ;
    rdfs:label "expression" ;
    transl_coher:involvesMultiple g1:number_1 ;
    transl_coher:involvesNoun g1:member_1 ;
    transl_coher:differentExpression :expression_2 .

:expression_2 a transl_coher:Expression ;
    rdfs:label "expression" ;
    transl_coher:involvesNoun g2:member_1 ;
    quant:hasQuantifier quant:multiple,
        quant:some .