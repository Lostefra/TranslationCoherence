@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_it_en_sentence3: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence3/> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .

vn.role:Proposition rdfs:label "for" .

en_it_en_sentence3:During rdfs:label "during" .

en_it_en_sentence3:Few rdfs:label "few" .

en_it_en_sentence3:On rdfs:label "on" .

en_it_en_sentence3:Over rdfs:label "over" .

en_it_en_sentence3:ask_1 a en_it_en_sentence3:Ask ;
    rdfs:label "asked" ;
    vn.role:Agent en_it_en_sentence3:person_1 ;
    vn.role:Proposition en_it_en_sentence3:debate_1 ;
    en_it_en_sentence3:during en_it_en_sentence3:session_1 ;
    en_it_en_sentence3:over en_it_en_sentence3:day_1 .

en_it_en_sentence3:Ask rdfs:label "asked" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence3:Day rdfs:label "days" .

en_it_en_sentence3:Debate rdfs:label "debate" .

en_it_en_sentence3:FewDay rdfs:subClassOf en_it_en_sentence3:Day .

en_it_en_sentence3:Next rdfs:label "next" .

en_it_en_sentence3:NextFewDay rdfs:subClassOf en_it_en_sentence3:FewDay .

en_it_en_sentence3:Person rdfs:label "You" .

en_it_en_sentence3:Session rdfs:label "session" .

en_it_en_sentence3:Subject rdfs:label "subject" .

en_it_en_sentence3:day_1 a en_it_en_sentence3:NextFewDay ;
    rdfs:label "days" ;
    dul:hasQuality en_it_en_sentence3:Next ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:few,
        quant:multiple .

en_it_en_sentence3:debate_1 a en_it_en_sentence3:Debate ;
    rdfs:label "debate" ;
    quant:hasDeterminer quant:a ;
    en_it_en_sentence3:on en_it_en_sentence3:subject_1 .

en_it_en_sentence3:person_1 a en_it_en_sentence3:Person ;
    rdfs:label "You" .

en_it_en_sentence3:session_1 a en_it_en_sentence3:Session ;
    rdfs:label "session" ;
    quant:hasDeterminer quant:this .

en_it_en_sentence3:subject_1 a en_it_en_sentence3:Subject ;
    rdfs:label "subject" ;
    quant:hasDeterminer quant:this .

