@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en__sentence5/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .

en_sentence5:For rdfs:label "for" .

en_sentence5:Of rdfs:label "'s" .

en_sentence5:please_1 a en_sentence5:Please ;
    rdfs:label "Please" ;
    vn.role:Cause en_sentence5:thing_1 ;
    vn.role:Experiencer en_sentence5:rise_1 .

en_sentence5:silence_1 a en_sentence5:Silence ;
    rdfs:label "silence" .

en_sentence5:Minute rdfs:label "minute" .

en_sentence5:Please rdfs:label "Please" ;
    rdfs:subClassOf dul:Event .

en_sentence5:Rise rdfs:label "rise" .

en_sentence5:Silence rdfs:label "silence" .

en_sentence5:Then rdfs:label "then" .

en_sentence5:minute_1 a en_sentence5:Minute ;
    rdfs:label "minute" ;
    quant:hasDeterminer quant:this .

en_sentence5:rise_1 a en_sentence5:Rise ;
    rdfs:label "rise" ;
    dul:hasQuality en_sentence5:Then ;
    en_sentence5:forSilenceOf en_sentence5:minute_1 .

en_sentence5:thing_1 a owl:Thing .

