@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix en_it_en_sentence5: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/ontology/en_it_en__sentence5/> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .

vn.role:Location rdfs:label "for" .

en_it_en_sentence5:Of rdfs:label "'s" .

en_it_en_sentence5:ask_1 a en_it_en_sentence5:ThereforeAsk ;
    rdfs:label "ask" ;
    boxing:hasModality boxing:Necessary ;
    dul:hasQuality en_it_en_sentence5:Therefore ;
    vn.role:Agent en_it_en_sentence5:person_1 ;
    vn.role:Topic en_it_en_sentence5:rise_1 .

en_it_en_sentence5:Ask rdfs:label "ask" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence5:Minute rdfs:label "minute" .

en_it_en_sentence5:Rise rdfs:label "rise" ;
    rdfs:subClassOf dul:Event .

en_it_en_sentence5:Silence rdfs:label "silence" .

en_it_en_sentence5:Therefore rdfs:label "therefore" .

en_it_en_sentence5:ThereforeAsk rdfs:label "Therefore Ask" ;
    rdfs:subClassOf en_it_en_sentence5:Ask .

en_it_en_sentence5:minute_1 a en_it_en_sentence5:Minute ;
    rdfs:label "minute" ;
    quant:hasDeterminer quant:this .

en_it_en_sentence5:person_1 a en_it_en_sentence5:Person ;
    rdfs:label "I" .

en_it_en_sentence5:person_2 a en_it_en_sentence5:Person ;
    rdfs:label "you" .

en_it_en_sentence5:rise_1 a en_it_en_sentence5:Rise ;
    rdfs:label "rise" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent en_it_en_sentence5:person_2 ;
    vn.role:Location en_it_en_sentence5:silence_1 .

en_it_en_sentence5:silence_1 a en_it_en_sentence5:Silence ;
    rdfs:label "silence" ;
    en_it_en_sentence5:silenceOf en_it_en_sentence5:minute_1 .

en_it_en_sentence5:Person rdfs:label "I",
        "you" .

