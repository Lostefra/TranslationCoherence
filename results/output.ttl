@prefix g1: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/EuroParl/Paragraph1/turtle/prova/g1.ttl#> .
@prefix g2: <https://raw.githubusercontent.com/Lostefra/TranslationCoherence/main/EuroParl/Paragraph1/turtle/prova/g2.ttl#> .

transl_coher:involves_mult a owl:ObjectProperty .

transl_coher:involves_node a owl:ObjectProperty .

transl_coher:expression_1 transl_coher:involves_mult g1:number_1 ;
    transl_coher:involves_node g1:member_1 ;
    transl_coher:same_expression transl_coher:expression_2 .

transl_coher:expression_2 transl_coher:involves_node g2:member_1 ;
    transl_coher:involves_quant quant:multiple,
        quant:some .