translation_coherence:nl_sentence427.ttl a owl:Ontology ;
    rdfs:label "nl_sentence427" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "nl_sentence427" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:In rdfs:label "in" .

ns1:President a ns1:Topic ;
    rdfs:label "President" ;
    boxer:possibleType owl:Thing ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister>,
        ns1:Commissioner,
        ns1:half_1 ;
    owl:sameAs dbpedia:President .

ns1:Rel rdfs:label "," .

ns1:congratulate_1 a ns1:Congratulate ;
    rdfs:label "congratulate" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Predicate ns1:half_1 ;
    vn.role:Theme ns1:Schroedter .

ns1:docuverse earmark:hasContent "Mr. President, Commissioner, in this moment and a half I should congratulate Mrs. Schroedter."^^xsd:string .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> a ns1:Topic ;
    rdfs:label "Mr." ;
    boxer:possibleType dul:Role .

ns1:Commissioner rdfs:label "Commissioner" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:Commissioner ;
    ns1:in ns1:moment_1 .

ns1:Congratulate rdfs:label "congratulate" ;
    rdfs:subClassOf dul:Event .

ns1:Half rdfs:label "half" ;
    rdfs:subClassOf dul:Amount .

ns1:Moment a owl:Class ;
    rdfs:label "moment" ;
    rdfs:subClassOf dul:Amount ;
    owl:equivalentClass dbpedia:In_This_Moment .

ns1:Person rdfs:label "I" .

ns1:Schroedter rdfs:label "Schroedter" ;
    boxer:possibleType foaf:Person ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> ;
    dul:hasRole <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> .

ns1:moment_1 a ns1:Moment ;
    rdfs:label "moment" ;
    quant:hasDeterminer quant:this .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> rdfs:label "Mrs." ;
    boxer:possibleType dul:Role .

ns1:Topic rdfs:label "Mr.",
        "President" .

ns1:half_1 a ns1:Half ;
    rdfs:label "half" ;
    quant:hasDeterminer quant:a .

