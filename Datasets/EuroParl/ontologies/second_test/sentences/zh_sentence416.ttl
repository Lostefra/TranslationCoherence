translation_coherence:zh_sentence416.ttl a owl:Ontology ;
    rdfs:label "zh_sentence416" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "zh_sentence416" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "I have just raised this issue and reiterate the position of Parliament."^^xsd:string .

ns1:raise_1 a ns1:JustRaise ;
    rdfs:label "raised" ;
    dul:hasQuality ns1:Just ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Theme ns1:issue_1 .

ns1:reiterate_1 a ns1:Reiterate ;
    rdfs:label "reiterate" ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Topic ns1:position_1 .

ns1:Issue rdfs:label "issue" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Topic> .

ns1:Just rdfs:label "just" .

ns1:JustRaise rdfs:subClassOf ns1:Raise .

ns1:Parliament rdfs:label "Parliament" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:Parliament .

ns1:Person rdfs:label "I" .

ns1:Position rdfs:label "position" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:Raise rdfs:label "raised" ;
    rdfs:subClassOf dul:Event .

ns1:Reiterate rdfs:label "reiterate" ;
    rdfs:subClassOf dul:Event .

ns1:issue_1 a ns1:Issue ;
    rdfs:label "issue" ;
    quant:hasDeterminer quant:this .

ns1:position_1 a ns1:Position ;
    rdfs:label "position" ;
    quant:hasDeterminer quant:the ;
    ns1:positionOf ns1:Parliament .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

