ns1:In rdfs:label "in" .

ns1:More rdfs:label "more" .

ns1:docuverse earmark:hasContent "Regional disputes are more important in the EU."^^xsd:string .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Important,
        ns1:dispute_1 ;
    ns1:in ns1:Eu .

ns1:Dispute rdfs:label "disputes" .

ns1:Eu rdfs:label "EU" ;
    boxer:possibleType foaf:Organisation .

ns1:Regional rdfs:label "Regional" .

ns1:RegionalDispute rdfs:subClassOf ns1:Dispute .

ns1:dispute_1 a ns1:RegionalDispute ;
    rdfs:label "disputes" ;
    dul:hasQuality ns1:Important,
        ns1:Regional ;
    quant:hasQuantifier quant:multiple ;
    ns1:more ns1:thing_1 .

ns1:thing_1 a owl:Thing .

ns1:Important rdfs:label "important" .

