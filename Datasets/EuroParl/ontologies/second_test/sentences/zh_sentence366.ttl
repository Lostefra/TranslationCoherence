    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
