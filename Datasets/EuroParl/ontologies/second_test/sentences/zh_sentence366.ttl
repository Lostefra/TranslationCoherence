ns1:Of rdfs:label "her" .

ns1:Rel rdfs:label "," .

ns1:docuverse earmark:hasContent "I would like to thank Mrs. Sholod for her excellent report, Mr. President."^^xsd:string .

ns1:like_1 a ns1:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Experiencer ns1:person_1 ;
    vn.role:Theme ns1:thank_1 .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> rdfs:label "Mr." ;
    boxer:possibleType dul:Role .

ns1:Excellent a owl:Class ;
    rdfs:label "excellent" ;
    rdfs:subClassOf dul:Quality .

ns1:ExcellentReport a owl:Class ;
    rdfs:label "excellent report" ;
    dul:hasQuality ns1:Excellent ;
    rdfs:subClassOf ns1:Report .

ns1:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

ns1:Person rdfs:label "I" .

ns1:President rdfs:label "President" ;
    boxer:possibleType owl:Thing ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> ;
    owl:sameAs dbpedia:President .

ns1:Report a owl:Class ;
    rdfs:label "report" ;
    rdfs:subClassOf dul:InformationEntity .

ns1:Sholod rdfs:label "Sholod" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> ;
    dul:hasRole <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> .

ns1:Thank rdfs:label "thank" ;
    rdfs:subClassOf dul:Event .

ns1:female_1 a ns1:Female ;
    rdfs:label "her" .

ns1:report_1 a ns1:ExcellentReport ;
    rdfs:label "report" ;
    dul:associatedWith ns1:President ;
    ns1:excellentReportOf ns1:female_1 .

ns1:thank_1 a ns1:Thank ;
    rdfs:label "thank" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Predicate ns1:report_1 ;
    vn.role:Theme ns1:Sholod .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> rdfs:label "Mrs." ;
    boxer:possibleType dul:Role .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

