ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "This minute of silence was then requested."^^xsd:string .

ns1:request_1 a ns1:ThenRequest ;
    rdfs:label "requested" ;
    dul:hasQuality ns1:Then ;
    vn.role:Proposition ns1:minute_1 .

ns1:Minute rdfs:label "minute" ;
    rdfs:subClassOf dul:Amount .

ns1:Request rdfs:label "requested" ;
    rdfs:subClassOf dul:Event .

ns1:Silence rdfs:label "silence" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Characteristic> .

ns1:Then rdfs:label "then" .

ns1:ThenRequest rdfs:subClassOf ns1:Request .

ns1:minute_1 a ns1:Minute ;
    rdfs:label "minute" ;
    quant:hasDeterminer quant:this ;
    ns1:minuteOf ns1:silence_1 .

ns1:silence_1 a ns1:Silence ;
    rdfs:label "silence" .

