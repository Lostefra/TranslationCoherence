translation_coherence:ko_sentence85.ttl a owl:Ontology ;
    rdfs:label "ko_sentence85" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "ko_sentence85" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Noteq rdfs:label "different",
        "other" .

ns1:docuverse earmark:hasContent "All other things were different opinions. It was crystal."^^xsd:string .

ns1:neuter_1 a owl:Thing ;
    rdfs:label "It" ;
    dul:hasQuality ns1:Crystal .

ns1:opinion_2 rdfs:label "opinions" .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:thing_2 ;
    ns1:entails ns1:situation_2 .

ns1:Crystal rdfs:label "crystal" .

ns1:Opinion rdfs:label "opinions" .

ns1:situation_2 a ns1:Situation ;
    boxing:involves ns1:thing_2 .

owl:Thing rdfs:label "It",
        "things" .

ns1:thing_2 a owl:Thing,
        ns1:Opinion ;
    rdfs:label "things" ;
    quant:hasDeterminer quant:other ;
    quant:hasQuantifier quant:all,
        quant:multiple .

