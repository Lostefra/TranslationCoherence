translation_coherence:it_sentence571.ttl a owl:Ontology ;
    rdfs:label "it_sentence571" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "it_sentence571" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:Social a ns1:EconomicSituation ;
    rdfs:label "Social",
        "situation" ;
    boxer:possibleType owl:Thing ;
    dul:hasQuality ns1:Topic ;
    owl:sameAs dbpedia:Social,
        dbpedia:Social_science .

ns1:development_1 a ns1:Development ;
    rdfs:label "development" ;
    dul:hasQuality ns1:Topic ;
    ns1:developmentOf ns1:region_1 .

ns1:docuverse earmark:hasContent "Social and economic situation and development of the regions of the Union"^^xsd:string .

ns1:situation_1 rdfs:label "situation" .

ns1:Development a owl:Class ;
    rdfs:label "development" ;
    rdfs:subClassOf dul:Situation ;
    owl:equivalentClass dbpedia:Economic_development .

ns1:Economic a owl:Class ;
    rdfs:label "economic" ;
    rdfs:subClassOf dul:Quality ;
    owl:equivalentClass dbpedia:Economy .

ns1:EconomicSituation a owl:Class ;
    rdfs:label "economic situation" ;
    dul:hasQuality ns1:Economic ;
    rdfs:subClassOf ns1:Situation ;
    owl:equivalentClass dbpedia:Great_Recession .

ns1:Region rdfs:label "regions" .

ns1:Situation a owl:Class ;
    rdfs:label "Social",
        "situation" ;
    rdfs:subClassOf dul:Situation .

ns1:Union rdfs:label "Union" ;
    boxer:possibleType foaf:Organisation .

ns1:region_1 a ns1:Region ;
    rdfs:label "regions" ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple ;
    ns1:regionOf ns1:Union .

