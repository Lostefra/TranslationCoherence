@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vn.role:Theme rdfs:label "on" .

translation_coherence:it_sentence549.ttl a owl:Ontology ;
    rdfs:label "it_sentence549" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "it_sentence549" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "Finally, I would like to focus on some of the challenges you have reiterated, Mrs Schroedter."^^xsd:string .

ns1:like_1 a ns1:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Experiencer ns1:person_1 ;
    vn.role:Theme ns1:situation_1 .

ns1:reiterate_1 a ns1:Reiterate ;
    rdfs:label "reiterated" ;
    dul:hasQuality ns1:Finally ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Topic ns1:Schroedter .

owl:Thing rdfs:label "some" .

ns1:Challenge rdfs:label "challenges" .

ns1:Finally rdfs:label "Finally" .

ns1:Focus rdfs:label "focus" ;
    rdfs:subClassOf dul:Event .

ns1:Have rdfs:label "have" ;
    rdfs:subClassOf dul:Event .

ns1:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

ns1:Reiterate rdfs:label "reiterated" ;
    rdfs:subClassOf dul:Event .

ns1:Schroedter rdfs:label "Schroedter" ;
    boxer:possibleType foaf:Person ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> ;
    dul:hasRole <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> .

ns1:focus_1 a ns1:Focus ;
    rdfs:label "focus" ;
    vn.role:Experiencer ns1:person_1 ;
    vn.role:Theme ns1:thing_1 .

ns1:have_1 a ns1:Have ;
    rdfs:label "have" ;
    vn.role:Agent ns1:person_2 ;
    vn.role:Patient ns1:challenge_1 .

ns1:person_2 a ns1:Person ;
    rdfs:label "you" .

ns1:situation_1 a ns1:Situation ;
    boxing:hasModality boxing:Necessary ;
    boxing:involves ns1:focus_1,
        ns1:have_1,
        ns1:thing_1 .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Missis> rdfs:label "Mrs" ;
    boxer:possibleType dul:Role .

ns1:Person rdfs:label "I",
        "you" .

ns1:challenge_1 a ns1:Challenge ;
    rdfs:label "challenges" ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple,
        quant:some .

ns1:thing_1 a owl:Thing ;
    rdfs:label "some" ;
    ns1:thingOf ns1:challenge_1 .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

