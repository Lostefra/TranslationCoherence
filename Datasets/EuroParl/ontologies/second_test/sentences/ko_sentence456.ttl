@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix coref: <http://www.ontologydesignpatterns.org/ont/cnlp/coref.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix earmark: <http://www.essepuntato.it/2008/12/earmark#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://w3id.org/stlab/ke/amiala/ko_sentence456.ttl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix translation_coherence: <https://w3id.org/stlab/ke/amiala/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dul:hasDataValue a owl:DataTypeProperty .

translation_coherence:ko_sentence456.ttl a owl:Ontology ;
    rdfs:label "ko_sentence456" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "ko_sentence456" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:That rdfs:label "that" .

ns1:docuverse earmark:hasContent "Therefore, I have two points that I would like to attract the attention of the Committee."^^xsd:string .

ns1:have_1 a ns1:Have ;
    rdfs:label "have" ;
    dul:hasQuality ns1:Therefore ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Patient ns1:point_1 .

ns1:Attention rdfs:label "attention" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#CognitiveEntity> .

ns1:Attract rdfs:label "attract" ;
    rdfs:subClassOf dul:Event .

ns1:Committee rdfs:label "Committee" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:Committee .

ns1:Have rdfs:label "have" ;
    rdfs:subClassOf dul:Event .

ns1:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

ns1:Point rdfs:label "points" .

ns1:Therefore rdfs:label "Therefore" .

ns1:attention_1 a ns1:Attention ;
    rdfs:label "attention" ;
    quant:hasDeterminer quant:the ;
    ns1:attentionOf ns1:Committee .

ns1:attract_1 a ns1:Attract ;
    rdfs:label "attract" ;
    boxer:agent ns1:person_2 ;
    boxer:patient ns1:attention_1 ;
    boxing:hasModality boxing:Necessary .

ns1:like_1 a ns1:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Experiencer ns1:person_2 ;
    vn.role:Theme ns1:attract_1 .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

ns1:point_1 a ns1:Point ;
    rdfs:label "points" ;
    dul:hasDataValue "2"^^xsd:nonNegativeInteger ;
    quant:hasQuantifier quant:multiple,
        quant:two ;
    ns1:that ns1:like_1 .

ns1:Person rdfs:label "I" .

ns1:person_2 a ns1:Person ;
    rdfs:label "I" .

