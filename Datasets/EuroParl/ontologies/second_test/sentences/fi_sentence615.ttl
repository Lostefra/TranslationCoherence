ns1:Of rdfs:label "'s" .

ns1:docuverse earmark:hasContent "It is supported by the European_Union's social funds."^^xsd:string .

ns1:support_1 a ns1:Support ;
    rdfs:label "supported" ;
    vn.role:Agent ns1:fund_1 ;
    vn.role:Beneficiary ns1:neuter_1 .

owl:Thing rdfs:label "It" .

ns1:European_union rdfs:label "European Union" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:European_Union .

ns1:Fund rdfs:label "funds" .

ns1:Social rdfs:label "social" ;
    owl:sameAs dbpedia:Social_issue .

ns1:SocialFund rdfs:subClassOf ns1:Fund .

ns1:Support rdfs:label "supported" ;
    rdfs:subClassOf dul:Event .

ns1:fund_1 a ns1:SocialFund ;
    rdfs:label "funds" ;
    dul:hasQuality ns1:Social ;
    quant:hasQuantifier quant:multiple ;
    ns1:socialFundOf ns1:European_union .

ns1:neuter_1 a owl:Thing ;
    rdfs:label "It" .

