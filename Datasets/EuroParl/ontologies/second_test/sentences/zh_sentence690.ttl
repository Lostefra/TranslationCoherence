@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix coref: <http://www.ontologydesignpatterns.org/ont/cnlp/coref.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix earmark: <http://www.essepuntato.it/2008/12/earmark#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://w3id.org/stlab/ke/amiala/zh_sentence690.ttl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix translation_coherence: <https://w3id.org/stlab/ke/amiala/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

translation_coherence:zh_sentence690.ttl a owl:Ontology ;
    rdfs:label "zh_sentence690" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "zh_sentence690" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "Mr. Francois has just said that the results of growth are uneven."^^xsd:string .

ns1:say_1 a ns1:JustSay ;
    rdfs:label "said" ;
    dul:hasQuality ns1:Just ;
    vn.role:Agent ns1:Francois ;
    vn.role:Topic ns1:situation_1 .

ns1:Francois rdfs:label "Francois" ;
    boxer:possibleType foaf:Person ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> ;
    dul:hasRole <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> .

ns1:Growth rdfs:label "growth" ;
    rdfs:subClassOf dul:Process .

ns1:Just rdfs:label "just" .

ns1:JustSay rdfs:subClassOf ns1:Say .

ns1:Result rdfs:label "results" .

ns1:Say rdfs:label "said" ;
    rdfs:subClassOf dul:Event .

ns1:result_1 a ns1:Result ;
    rdfs:label "results" ;
    dul:hasQuality ns1:Uneven ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple ;
    ns1:resultOf ns1:growth_1 .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Uneven,
        ns1:growth_1,
        ns1:result_1 .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> rdfs:label "Mr." ;
    boxer:possibleType dul:Role .

ns1:Uneven rdfs:label "uneven" .

ns1:growth_1 a ns1:Growth ;
    rdfs:label "growth" .

