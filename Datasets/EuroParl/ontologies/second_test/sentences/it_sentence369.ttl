translation_coherence:it_sentence369.ttl a owl:Ontology ;
    rdfs:label "it_sentence369" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "it_sentence369" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:In rdfs:label "in" .

ns1:docuverse earmark:hasContent "We are very behind in this matter."^^xsd:string .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Behind,
        ns1:Very,
        ns1:person_1 ;
    ns1:in ns1:matter_1 .

ns1:Matter a owl:Class ;
    rdfs:label "matter" ;
    rdfs:subClassOf dul:Situation ;
    owl:equivalentClass dbpedia:Matter_(novel) .

ns1:Person a owl:Class ;
    rdfs:label "We" ;
    owl:equivalentClass dbpedia:We_(novel) .

ns1:matter_1 a ns1:Matter ;
    rdfs:label "matter" ;
    quant:hasDeterminer quant:this .

ns1:person_1 a ns1:Person ;
    rdfs:label "We" ;
    dul:hasQuality ns1:Behind,
        ns1:Very .

ns1:Behind rdfs:label "behind" .

ns1:Very rdfs:label "very" .

