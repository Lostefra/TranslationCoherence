ns1:Behind rdfs:label "behind" .

ns1:On rdfs:label "on" .

ns1:docuverse earmark:hasContent "We are now lagging behind on this issue."^^xsd:string .

ns1:lag_1 a ns1:NowLag ;
    rdfs:label "lagging" ;
    boxer:agent ns1:person_1 ;
    dul:hasQuality ns1:Now ;
    ns1:behindOn ns1:issue_1 .

ns1:Issue rdfs:label "issue" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Topic> .

ns1:Lag rdfs:label "lagging" ;
    rdfs:subClassOf dul:Event .

ns1:Now rdfs:label "now" .

ns1:NowLag rdfs:subClassOf ns1:Lag .

ns1:Person rdfs:label "We" .

ns1:issue_1 a ns1:Issue ;
    rdfs:label "issue" ;
    quant:hasDeterminer quant:this .

ns1:person_1 a ns1:Person ;
    rdfs:label "We" .

