ns1:As rdfs:label "As" .

ns1:Rel rdfs:label "," .

ns1:docuverse earmark:hasContent "As I have limited time, Mr. President, I will stand straight."^^xsd:string .

ns1:limit_1 a ns1:Limit ;
    rdfs:label "limited" ;
    vn.role:Cause ns1:person_1 ;
    vn.role:Patient ns1:time_1 ;
    ns1:as ns1:stand_1 .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> rdfs:label "Mr." ;
    boxer:possibleType dul:Role .

ns1:Limit rdfs:label "limited" ;
    rdfs:subClassOf dul:Event .

ns1:President rdfs:label "President" ;
    boxer:possibleType owl:Thing ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> ;
    owl:sameAs dbpedia:President .

ns1:Stand rdfs:label "stand" ;
    rdfs:subClassOf dul:Event .

ns1:Straight rdfs:label "straight" .

ns1:Time rdfs:label "time" ;
    rdfs:subClassOf dul:Situation .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

ns1:person_2 a ns1:Person ;
    rdfs:label "I" .

ns1:stand_1 a ns1:Stand ;
    rdfs:label "stand" ;
    boxing:hasModality boxing:Necessary ;
    dul:hasQuality ns1:Straight ;
    vn.role:Agent ns1:person_2 .

ns1:time_1 a ns1:Time ;
    rdfs:label "time" ;
    dul:associatedWith ns1:President .

ns1:Person rdfs:label "I" .

