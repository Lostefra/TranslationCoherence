ns1:For rdfs:label "for" .

ns1:docuverse earmark:hasContent "Personally, I am completely for the guidelines."^^xsd:string .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:guideline_1,
        ns1:person_1 ;
    dul:hasQuality ns1:Completely,
        ns1:Personally .

ns1:Completely rdfs:label "completely" .

ns1:Guideline rdfs:label "guidelines" .

ns1:Person rdfs:label "I" .

ns1:Personally rdfs:label "Personally" .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" ;
    ns1:for ns1:guideline_1 .

ns1:guideline_1 a ns1:Guideline ;
    rdfs:label "guidelines" ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple .

