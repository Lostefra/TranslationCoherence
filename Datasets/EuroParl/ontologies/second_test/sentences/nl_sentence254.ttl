ns1:In rdfs:label "in" .

ns1:Of rdfs:label "'s",
        "my" .

ns1:docuverse earmark:hasContent "I hope my proposal is considered in tomorrow's vote."^^xsd:string .

ns1:hope_1 a ns1:Hope ;
    rdfs:label "hope" ;
    vn.role:Experiencer ns1:person_2 ;
    vn.role:Theme ns1:consider_1 .

ns1:person_1 coref:coref ns1:person_2 .

ns1:vote_1 a ns1:Vote ;
    rdfs:label "vote" .

ns1:Consider rdfs:label "considered" ;
    rdfs:subClassOf dul:Event .

ns1:Hope rdfs:label "hope" ;
    rdfs:subClassOf dul:Event .

ns1:Person rdfs:label "I" .

ns1:Proposal rdfs:label "proposal" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Topic> .

ns1:Tomorrow rdfs:label "tomorrow" ;
    rdfs:subClassOf dul:Amount .

ns1:Vote rdfs:label "vote" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Activity> .

ns1:consider_1 a ns1:Consider ;
    rdfs:label "considered" ;
    vn.role:Theme ns1:proposal_1 ;
    ns1:inVoteOf ns1:tomorrow_1 .

ns1:proposal_1 a ns1:Proposal ;
    rdfs:label "proposal" ;
    ns1:proposalOf ns1:person_2 .

ns1:tomorrow_1 a ns1:Tomorrow ;
    rdfs:label "tomorrow" .

ns1:person_2 a ns1:Person ;
    rdfs:label "I",
        "my" .

