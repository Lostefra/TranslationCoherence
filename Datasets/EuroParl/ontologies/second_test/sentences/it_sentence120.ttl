@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix coref: <http://www.ontologydesignpatterns.org/ont/cnlp/coref.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix earmark: <http://www.essepuntato.it/2008/12/earmark#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://w3id.org/stlab/ke/amiala/it_sentence120.ttl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix translation_coherence: <https://w3id.org/stlab/ke/amiala/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vn.role:Predicate rdfs:label "for" .

translation_coherence:it_sentence120.ttl a owl:Ontology ;
    rdfs:label "it_sentence120" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "it_sentence120" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "Madam President, I would like to thank Mr Poettering for the publicity of this debate."^^xsd:string .

ns1:like_1 a ns1:Like ;
    rdfs:label "like" ;
    boxing:hasModality boxing:Necessary ;
    dul:associatedWith ns1:President ;
    vn.role:Experiencer ns1:person_1 ;
    vn.role:Theme ns1:thank_1 .

<http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> rdfs:label "Mr" ;
    boxer:possibleType dul:Role .

ns1:Debate rdfs:label "debate" ;
    rdfs:subClassOf dul:InformationEntity .

ns1:Like rdfs:label "like" ;
    rdfs:subClassOf dul:Event .

ns1:Madam a ns1:Topic ;
    rdfs:label "Madam" ;
    boxer:possibleType foaf:Organisation .

ns1:Person rdfs:label "I" .

ns1:Poettering rdfs:label "Poettering" ;
    boxer:possibleType owl:Thing ;
    dul:associatedWith <http://www.ontologydesignpatterns.org/ont/boxer/title.owl#Mister> .

ns1:President a ns1:Topic ;
    rdfs:label "President" ;
    boxer:possibleType owl:Thing ;
    dul:associatedWith ns1:Madam ;
    owl:sameAs dbpedia:President .

ns1:Publicity rdfs:label "publicity" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Topic> .

ns1:Thank rdfs:label "thank" ;
    rdfs:subClassOf dul:Event .

ns1:debate_1 a ns1:Debate ;
    rdfs:label "debate" ;
    quant:hasDeterminer quant:this .

ns1:publicity_1 a ns1:Publicity ;
    rdfs:label "publicity" ;
    quant:hasDeterminer quant:the ;
    ns1:publicityOf ns1:debate_1 .

ns1:thank_1 a ns1:Thank ;
    rdfs:label "thank" ;
    boxing:hasModality boxing:Necessary ;
    vn.role:Agent ns1:person_1 ;
    vn.role:Predicate ns1:publicity_1 ;
    vn.role:Theme ns1:Poettering .

ns1:Topic rdfs:label "Madam",
        "President" .

ns1:person_1 a ns1:Person ;
    rdfs:label "I" .

