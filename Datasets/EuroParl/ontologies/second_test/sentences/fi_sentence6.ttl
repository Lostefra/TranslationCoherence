ns1:In rdfs:label "in" .

ns1:Of rdfs:label "of" .

ns1:docuverse earmark:hasContent "Get up, in the silence of this moment."^^xsd:string .

ns1:get_1 a ns1:Get ;
    rdfs:label "Get" ;
    dul:hasQuality ns1:Up ;
    vn.role:Patient ns1:thing_1 ;
    ns1:inSilenceOf ns1:moment_1 .

ns1:silence_1 a ns1:Silence ;
    rdfs:label "silence" ;
    quant:hasDeterminer quant:the .

ns1:Get rdfs:label "Get" ;
    rdfs:subClassOf dul:Event .

ns1:Moment rdfs:label "moment" ;
    rdfs:subClassOf dul:Amount .

ns1:Silence rdfs:label "silence" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Characteristic> .

ns1:Up rdfs:label "up" .

ns1:moment_1 a ns1:Moment ;
    rdfs:label "moment" ;
    quant:hasDeterminer quant:this .

ns1:thing_1 a owl:Thing .

