translation_coherence:it_sentence416.ttl a owl:Ontology ;
    rdfs:label "it_sentence416" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
