ns1:About rdfs:label "about" .

ns1:Of rdfs:label "'s",
        "my" .

ns1:docuverse earmark:hasContent "I repeat my concern about Parliament's position."^^xsd:string .

ns1:person_1 coref:coref ns1:person_2 .

ns1:repeat_1 a ns1:Repeat ;
    rdfs:label "repeat" ;
    vn.role:Agent ns1:person_2 ;
    vn.role:Theme ns1:concern_1 .

ns1:Concern rdfs:label "concern" ;
    rdfs:subClassOf dul:Situation .

ns1:Parliament rdfs:label "Parliament" ;
    boxer:possibleType foaf:Organisation ;
    owl:sameAs dbpedia:Parliament .

ns1:Person rdfs:label "I" .

ns1:Position rdfs:label "position" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:Repeat rdfs:label "repeat" ;
    rdfs:subClassOf dul:Event .

ns1:concern_1 a ns1:Concern ;
    rdfs:label "concern" ;
    ns1:about ns1:position_1 ;
    ns1:concernOf ns1:person_2 .

ns1:position_1 a ns1:Position ;
    rdfs:label "position" ;
    ns1:positionOf ns1:Parliament .

ns1:person_2 a ns1:Person ;
    rdfs:label "I",
        "my" .

