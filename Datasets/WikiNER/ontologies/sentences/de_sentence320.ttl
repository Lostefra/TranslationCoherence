translation_coherence:de_sentence320.ttl a owl:Ontology ;
    rdfs:label "de_sentence320" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "de_sentence320" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:By rdfs:label "by" .

ns1:docuverse earmark:hasContent "The Whigs were unavoidable by the US-Nebraska law."^^xsd:string .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Unavoidable,
        ns1:whig_1 ;
    ns1:situationBy ns1:law_1 .

ns1:Law rdfs:label "law" ;
    rdfs:subClassOf dul:InformationEntity .

ns1:Us-nebraska rdfs:label "US-Nebraska" ;
    boxer:possibleType foaf:Organisation .

ns1:Whig a owl:Class ;
    rdfs:label "Whigs" ;
    owl:equivalentClass dbpedia:Whigs_(British_political_party) .

ns1:law_1 a ns1:Law ;
    rdfs:label "law" ;
    dul:associatedWith ns1:Us-nebraska .

ns1:whig_1 a ns1:Whig ;
    rdfs:label "Whigs" ;
    dul:hasQuality ns1:Unavoidable ;
    quant:hasDeterminer quant:the ;
    quant:hasQuantifier quant:multiple .

ns1:Unavoidable rdfs:label "unavoidable" .

