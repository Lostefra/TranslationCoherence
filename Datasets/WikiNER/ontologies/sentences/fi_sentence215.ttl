ns1:When rdfs:label "When" .

ns1:docuverse earmark:hasContent "When he realized that the battle was inevitable."^^xsd:string .

ns1:realize_1 a ns1:Realize ;
    rdfs:label "realized" ;
    vn.role:Agent ns1:male_1 ;
    vn.role:Theme ns1:situation_2 ;
    ns1:when ns1:situation_1 .

ns1:Battle rdfs:label "battle" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Activity> .

ns1:Male rdfs:label "he" .

ns1:Realize rdfs:label "realized" ;
    rdfs:subClassOf dul:Event .

ns1:battle_1 a ns1:Battle ;
    rdfs:label "battle" ;
    dul:hasQuality ns1:Inevitable ;
    quant:hasDeterminer quant:the .

ns1:event_1 a dul:Event .

ns1:male_1 a ns1:Male ;
    rdfs:label "he" .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:event_1 .

ns1:situation_2 a ns1:Situation ;
    boxing:involves ns1:Inevitable,
        ns1:battle_1 .

ns1:Inevitable rdfs:label "inevitable" .

