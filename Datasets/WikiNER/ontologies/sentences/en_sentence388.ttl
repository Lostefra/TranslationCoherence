ns1:Across rdfs:label "across" .

ns1:docuverse earmark:hasContent "Nevertheless, Republicans across the country feared that Lincoln would be defeated."^^xsd:string .

ns1:fear_1 a ns1:Fear ;
    rdfs:label "feared" ;
    dul:hasQuality ns1:Nevertheless ;
    vn.role:Experiencer ns1:Republicans ;
    vn.role:Theme ns1:defeat_1 .

dbpedia:Abraham_Lincoln a schema:Person .

ns1:Country rdfs:label "country" ;
    rdfs:subClassOf dul:Organization .

ns1:Defeat rdfs:label "defeated" ;
    rdfs:subClassOf dul:Event .

ns1:Fear rdfs:label "feared" ;
    rdfs:subClassOf dul:Event .

ns1:Lincoln rdfs:label "Lincoln" ;
    boxer:possibleType foaf:Person ;
    owl:sameAs dbpedia:Abraham_Lincoln .

ns1:Nevertheless rdfs:label "Nevertheless" .

ns1:Republicans rdfs:label "Republicans" ;
    boxer:possibleType owl:Thing ;
    owl:sameAs dbpedia:History_of_the_United_States_Republican_Party ;
    ns1:across ns1:country_1 .

ns1:country_1 a ns1:Country ;
    rdfs:label "country" ;
    quant:hasDeterminer quant:the .

ns1:defeat_1 a ns1:Defeat ;
    rdfs:label "defeated" ;
    boxer:patient ns1:Lincoln ;
    boxing:hasModality boxing:Necessary .

