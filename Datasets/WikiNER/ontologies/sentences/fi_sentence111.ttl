@prefix boxer: <http://www.ontologydesignpatterns.org/ont/boxer/boxer.owl#> .
@prefix boxing: <http://www.ontologydesignpatterns.org/ont/boxer/boxing.owl#> .
@prefix coref: <http://www.ontologydesignpatterns.org/ont/cnlp/coref.owl#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix earmark: <http://www.essepuntato.it/2008/12/earmark#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://w3id.org/stlab/ke/amiala/fi_sentence111.ttl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix translation_coherence: <https://w3id.org/stlab/ke/amiala/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.ontologydesignpatterns.org/ont/fred/domain.owl#%5Elargest> rdfs:label "largest" .

translation_coherence:fi_sentence111.ttl a owl:Ontology ;
    rdfs:label "fi_sentence111" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "fi_sentence111" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:Eq rdfs:label "is" .

ns1:In rdfs:label "in" .

ns1:disaster_1 a owl:Thing,
        ns1:LargestNaturalDisaster,
        ns1:NaturalDisaster ;
    rdfs:label "disaster" ;
    dul:hasQuality ns1:Largest ;
    quant:hasDeterminer quant:the,
        quant:this ;
    ns1:largestAmong ns1:NaturalDisaster ;
    ns1:locatedIn ns1:Alabama .

ns1:docuverse earmark:hasContent "This is the largest natural disaster in Alabama."^^xsd:string .

ns1:thing_1 rdfs:label "This" .

dbpedia:Alabama a schema:AdministrativeArea,
        schema:Place .

owl:Thing rdfs:label "This" ;
    rdfs:subClassOf ns1:LargestNaturalDisaster .

ns1:Alabama rdfs:label "Alabama" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    owl:sameAs dbpedia:Alabama .

ns1:Disaster a owl:Class ;
    rdfs:label "disaster" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Event> .

ns1:Largest rdfs:label "largest" .

ns1:Natural a owl:Class ;
    rdfs:label "natural" ;
    rdfs:subClassOf dul:Quality .

ns1:LargestNaturalDisaster a owl:Class ;
    rdfs:label "largest natural disaster" ;
    rdfs:subClassOf ns1:NaturalDisaster .

ns1:NaturalDisaster a owl:Class ;
    rdfs:label "natural disaster" ;
    dul:hasQuality ns1:Natural ;
    rdfs:subClassOf ns1:Disaster ;
    owl:equivalentClass dbpedia:Natural_disaster .

