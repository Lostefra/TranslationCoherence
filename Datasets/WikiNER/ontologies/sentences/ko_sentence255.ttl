ns1:In rdfs:label "in" .

ns1:docuverse earmark:hasContent "Achilles is often mentioned in music."^^xsd:string .

ns1:mention_1 a ns1:OftenMention ;
    rdfs:label "mentioned" ;
    dul:hasQuality ns1:Often ;
    vn.role:Topic ns1:Achilles ;
    ns1:in ns1:music_1 .

ns1:Achilles rdfs:label "Achilles" ;
    boxer:possibleType foaf:Person ;
    owl:sameAs dbpedia:Achilles .

ns1:Mention rdfs:label "mentioned" ;
    rdfs:subClassOf dul:Event .

ns1:Music rdfs:label "music" ;
    rdfs:subClassOf dul:InformationEntity .

ns1:Often rdfs:label "often" .

ns1:OftenMention rdfs:subClassOf ns1:Mention .

ns1:music_1 a ns1:Music ;
    rdfs:label "music" .

