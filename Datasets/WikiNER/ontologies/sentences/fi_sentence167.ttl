ns1:In rdfs:label "in" .

ns1:docuverse earmark:hasContent "There are 67 provinces in Alabama."^^xsd:string .

ns1:province_1 rdfs:label "provinces" .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Province,
        ns1:there_1 ;
    ns1:locatedIn ns1:Alabama .

dbpedia:Alabama a schema:AdministrativeArea,
        schema:Place .

ns1:Alabama rdfs:label "Alabama" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    owl:sameAs dbpedia:Alabama .

ns1:There rdfs:label "There" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:there_1 a ns1:Province,
        ns1:There ;
    rdfs:label "There" ;
    dul:hasDataValue "67"^^xsd:nonNegativeInteger ;
    quant:hasQuantifier quant:multiple .

ns1:Province rdfs:label "provinces" .

