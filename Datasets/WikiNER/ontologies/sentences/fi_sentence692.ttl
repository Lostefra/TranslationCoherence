ns1:So rdfs:label "so" .

ns1:docuverse earmark:hasContent "The threat was so severe that the island was uninhabited."^^xsd:string .

ns1:threat_1 a ns1:Threat ;
    rdfs:label "threat" ;
    dul:hasQuality ns1:Severe ;
    quant:hasDeterminer quant:the ;
    ns1:so ns1:situation_1 .

ns1:Island rdfs:label "island" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:Severe rdfs:label "severe" .

ns1:Threat rdfs:label "threat" ;
    rdfs:subClassOf dul:Description .

ns1:island_1 a ns1:Island ;
    rdfs:label "island" ;
    dul:hasQuality ns1:Uninhabited ;
    quant:hasDeterminer quant:the .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:Uninhabited,
        ns1:island_1 .

ns1:Uninhabited rdfs:label "uninhabited" .

