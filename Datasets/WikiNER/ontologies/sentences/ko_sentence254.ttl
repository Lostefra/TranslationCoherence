        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:As rdfs:label "as" .

ns1:Of rdfs:label "of" .

ns1:be_1 a ns1:Be ;
    rdfs:label "is" ;
    boxer:agent ns1:role_1 ;
    ns1:as ns1:follow_1 .

ns1:docuverse earmark:hasContent "The role of Achilles is as follows:"^^xsd:string .

ns1:Achilles rdfs:label "Achilles" ;
    boxer:possibleType foaf:Person ;
    owl:sameAs dbpedia:Achilles .

ns1:Be rdfs:label "is" ;
    rdfs:subClassOf dul:Event .

ns1:Follow rdfs:label "follows" ;
    rdfs:subClassOf dul:Event .

ns1:Role rdfs:label "role" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Activity> .

ns1:follow_1 a ns1:Follow ;
    rdfs:label "follows" ;
    vn.role:Theme ns1:thing_1 .

ns1:role_1 a ns1:Role ;
    rdfs:label "role" ;
    quant:hasDeterminer quant:the ;
    ns1:roleOf ns1:Achilles .

ns1:thing_1 a owl:Thing .

