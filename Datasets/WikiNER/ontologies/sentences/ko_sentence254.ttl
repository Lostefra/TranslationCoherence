translation_coherence:ko_sentence254.ttl a owl:Ontology ;
    rdfs:label "ko_sentence254" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "ko_sentence254" ;
