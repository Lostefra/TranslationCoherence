translation_coherence:de_sentence713.ttl a owl:Ontology ;
    rdfs:label "de_sentence713" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "de_sentence713" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:In rdfs:label "in" .

ns1:docuverse earmark:hasContent "Foreign_investment in Algeria has recently increased."^^xsd:string .

ns1:increase_1 a ns1:RecentlyIncrease ;
    rdfs:label "increased" ;
    dul:hasQuality ns1:Recently ;
    vn.role:Patient ns1:foreign_investment_1 .

dbpedia:Algeria a schema:Country,
        schema:Place .

ns1:Algeria rdfs:label "Algeria" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    owl:sameAs dbpedia:Algeria .

ns1:Foreign_investment a owl:Class ;
    rdfs:label "Foreign investment" ;
    owl:equivalentClass dbpedia:Foreign_direct_investment .

ns1:Increase rdfs:label "increased" ;
    rdfs:subClassOf dul:Event .

ns1:Recently rdfs:label "recently" .

ns1:RecentlyIncrease rdfs:subClassOf ns1:Increase .

ns1:foreign_investment_1 a ns1:Foreign_investment ;
    rdfs:label "Foreign investment" ;
    ns1:locatedIn ns1:Algeria .

