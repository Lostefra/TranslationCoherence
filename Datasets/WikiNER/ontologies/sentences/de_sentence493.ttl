ns1:Of rdfs:label "of" .

ns1:contest_1 a ns1:Contest ;
    rdfs:label "contested" ;
    boxer:patient ns1:root_1 .

ns1:docuverse earmark:hasContent "The root of the Oscar name is contested."^^xsd:string .

ns1:Contest rdfs:label "contested" ;
    rdfs:subClassOf dul:Event .

ns1:Name rdfs:label "name" ;
    rdfs:subClassOf dul:Description .

ns1:Oscar rdfs:label "Oscar" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> .

ns1:Root rdfs:label "root" ;
    rdfs:subClassOf dul:PhysicalObject .

ns1:name_1 a ns1:Name ;
    rdfs:label "name" ;
    dul:associatedWith ns1:Oscar .

ns1:root_1 a ns1:Root ;
    rdfs:label "root" ;
    quant:hasDeterminer quant:the ;
    ns1:rootOf ns1:name_1 .

