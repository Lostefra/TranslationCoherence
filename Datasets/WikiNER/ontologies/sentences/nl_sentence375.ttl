@prefix earmark: <http://www.essepuntato.it/2008/12/earmark#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns1: <https://w3id.org/stlab/ke/amiala/nl_sentence375.ttl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix quant: <http://www.ontologydesignpatterns.org/ont/fred/quantifiers.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix translation_coherence: <https://w3id.org/stlab/ke/amiala/> .
@prefix vn.role: <http://www.ontologydesignpatterns.org/ont/vn/abox/role/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vn.role:Recipient rdfs:label "By" .

translation_coherence:nl_sentence375.ttl a owl:Ontology ;
    rdfs:label "nl_sentence375" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "nl_sentence375" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:In rdfs:label "in" .

ns1:docuverse earmark:hasContent "By the war, Lincoln showed an intense interest in the military campaign."^^xsd:string .

ns1:show_1 a ns1:Show ;
    rdfs:label "showed" ;
    vn.role:Agent ns1:Lincoln ;
    vn.role:Patient ns1:interest_1 ;
    vn.role:Recipient ns1:war_1 .

dbpedia:Abraham_Lincoln a schema:Person .

ns1:Campaign a owl:Class ;
    rdfs:label "campaign" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Event> .

ns1:Intense a owl:Class ;
    rdfs:label "intense" ;
    rdfs:subClassOf dul:Quality .

ns1:IntenseInterest a owl:Class ;
    rdfs:label "intense interest" ;
    dul:hasQuality ns1:Intense ;
    rdfs:subClassOf ns1:Interest .

ns1:Interest a owl:Class ;
    rdfs:label "interest" ;
    rdfs:subClassOf dul:Situation .

ns1:Lincoln rdfs:label "Lincoln" ;
    boxer:possibleType foaf:Person ;
    owl:sameAs dbpedia:Abraham_Lincoln .

ns1:MilitaryCampaign a owl:Class ;
    rdfs:label "Military Campaign" ;
    dul:hasQuality ns1:Military ;
    rdfs:subClassOf ns1:Campaign ;
    owl:equivalentClass dbpedia:Military_campaign .

ns1:Show rdfs:label "showed" ;
    rdfs:subClassOf dul:Event .

ns1:War a owl:Class ;
    rdfs:label "war" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Activity> ;
    owl:equivalentClass dbpedia:War .

ns1:campaign_1 a ns1:MilitaryCampaign ;
    rdfs:label "campaign" ;
    dul:hasQuality ns1:Military ;
    quant:hasDeterminer quant:the .

ns1:interest_1 a ns1:IntenseInterest ;
    rdfs:label "interest" ;
    quant:hasDeterminer quant:an ;
    ns1:in ns1:campaign_1 .

ns1:war_1 a ns1:War ;
    rdfs:label "war" ;
    quant:hasDeterminer quant:the .

ns1:Military a owl:Class ;
    rdfs:label "military" ;
    rdfs:subClassOf dul:Quality .

