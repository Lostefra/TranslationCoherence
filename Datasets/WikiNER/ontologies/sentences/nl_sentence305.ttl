dbpedia:James_K._Polk a schema:Person .

ns1:Attractive a owl:Class ;
    rdfs:label "attractive" ;
    rdfs:subClassOf dul:Quality .

ns1:AttractiveRainbow a owl:Class ;
    rdfs:label "attractive rainbow" ;
