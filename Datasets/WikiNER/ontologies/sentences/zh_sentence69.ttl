ns1:In rdfs:label "In" .

ns1:Less rdfs:label "less" .

ns1:Than rdfs:label "than" .

ns1:docuverse earmark:hasContent "In the Antarctic, they were less than 0.8."^^xsd:string .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:thing_1,
        "0.8"^^xsd:nonNegativeInteger ;
    ns1:locatedIn ns1:Antarctic .

owl:Thing rdfs:label "they" .

ns1:Antarctic rdfs:label "Antarctic" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    owl:sameAs dbpedia:Antarctic .

ns1:thing_1 a owl:Thing ;
    rdfs:label "they" ;
    ns1:lessThan "0.8"^^xsd:nonNegativeInteger .

