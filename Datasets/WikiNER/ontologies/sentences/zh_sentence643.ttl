ns1:Of rdfs:label "'s" .

ns1:docuverse earmark:hasContent "Two films spoke about Rand's life."^^xsd:string .

ns1:speak_1 a ns1:Speak ;
    rdfs:label "spoke" ;
    vn.role:Agent ns1:film_1 ;
    vn.role:Topic ns1:life_1 .

ns1:Film rdfs:label "films" .

ns1:Life rdfs:label "life" ;
    rdfs:subClassOf dul:Situation .

ns1:Rand rdfs:label "Rand" ;
    boxer:possibleType foaf:Organisation .

ns1:Speak rdfs:label "spoke" ;
    rdfs:subClassOf dul:Event .

ns1:film_1 a ns1:Film ;
    rdfs:label "films" ;
    dul:hasDataValue "2"^^xsd:nonNegativeInteger ;
    quant:hasQuantifier quant:multiple,
        quant:two .

ns1:life_1 a ns1:Life ;
    rdfs:label "life" ;
    ns1:lifeOf ns1:Rand .

