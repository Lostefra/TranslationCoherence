ns1:In rdfs:label "in" .

ns1:Multiple rdfs:label "multiple" .

ns1:Shortages quant:hasQuantifier quant:multiple .

ns1:configure_1 a ns1:Configure ;
    rdfs:label "configured" ;
    boxer:patient ns1:Berbers ;
    ns1:in ns1:shortage_1 .

ns1:docuverse earmark:hasContent "Berbers has been configured in multiple shortages."^^xsd:string .

ns1:Berbers rdfs:label "Berbers" ;
    boxer:possibleType <http://dbpedia.org/ontology/Place> ;
    owl:sameAs dbpedia:Berbers .

ns1:Configure rdfs:label "configured" ;
    rdfs:subClassOf dul:Event .

ns1:MultipleShortage rdfs:subClassOf ns1:Shortage .

ns1:Shortage rdfs:label "shortages" .

ns1:shortage_1 a ns1:MultipleShortage ;
    rdfs:label "shortages" ;
    quant:hasQuantifier quant:multiple .

