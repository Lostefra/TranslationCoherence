@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dul:hasDataValue a owl:DataTypeProperty .

translation_coherence:zh_sentence503.ttl a owl:Ontology ;
    rdfs:label "zh_sentence503" ;
    dc:creator "Andrea Lavista",
        "Lorenzo Mario Amorosa",
        "Michele Iannello" ;
    dc:description "None" ;
    dc:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dc:title "zh_sentence503" ;
    dct:issued "2021-08-13" ;
    dct:license <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:modified "2021-11-17" ;
    rdfs:comment "None" ;
    owl:imports dbpedia:,
        schema:,
        boxer:,
        boxing:,
        coref:,
        dul:,
        quant:,
        vn.role:,
        rdf:,
        rdfs:,
        owl:,
        time:,
        foaf: .

ns1:In rdfs:label "In" .

ns1:docuverse earmark:hasContent "In 2007, there were more than 40 million United_States observers."^^xsd:string .

ns1:observer_1 rdfs:label "observers" ;
    quant:hasQuantifier quant:multiple .

ns1:situation_1 a ns1:Situation ;
    boxing:involves ns1:there_1 ;
    ns1:in "2007-01-01"^^xsd:date .

ns1:united_states_1 a ns1:United_states ;
    rdfs:label "United States" ;
    dul:associatedWith ns1:there_1 ;
    quant:hasQuantifier quant:million,
        quant:multiple .

dbpedia:United_States rdfs:subClassOf schema:Country,
        schema:Place .

ns1:More rdfs:label "more" .

ns1:Observer rdfs:label "observers" .

ns1:Than rdfs:label "than" .

ns1:There rdfs:label "there" ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:UnitedStates rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/d0.owl#Location> .

ns1:United_states a owl:Class ;
    rdfs:label "United States" ;
    rdfs:subClassOf ns1:UnitedStates ;
    owl:equivalentClass dbpedia:United_States .

ns1:United_statesObserver rdfs:subClassOf ns1:Observer .

ns1:there_1 a ns1:There,
        ns1:United_statesObserver ;
    rdfs:label "there" ;
    dul:hasDataValue "40000000"^^xsd:nonNegativeInteger ;
    dul:hasQuality ns1:More,
        ns1:Than .

